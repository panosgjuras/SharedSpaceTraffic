# Descriptive statistics before - after
# number of pedestrian crossings per 2 minutes
describe(subset(ped_data[-c(155,154,191,166, 33, 31, 1, 27, 69, 98, 51), ], shared=="shnaf")$cross) # descriptive statistics, not shared
# Descriptive statistics before - after
